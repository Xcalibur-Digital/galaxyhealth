{
  "name": "galaxy-health",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "^2.0.1"
  },
  "resolutions": {
    "glob": "^10.3.10",
    "inflight": "^2.0.1"
  },
  "scripts": {
    "dev": "vite",
    "dev:debug": "set DEBUG=vite:* && vite --debug",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json}",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "seed": "node scripts/seedRecommendations.js",
    "init-store": "node scripts/initializeStore.js",
    "init-data": "node scripts/initializeData.js",
    "clean": "if exist .turbo rmdir /s /q .turbo && if exist node_modules rmdir /s /q node_modules && if exist dist rmdir /s /q dist"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@mantine/core": "^7.3.2",
    "@mantine/dropzone": "^7.3.2",
    "@mantine/hooks": "^7.3.2",
    "@mantine/notifications": "^7.3.2",
    "@mantine/dates": "^7.3.2",
    "mantine-datatable": "^7.3.2",
    "postcss": "^8.4.33",
    "postcss-preset-mantine": "^1.12.3",
    "postcss-simple-vars": "^7.0.1",
    "@tabler/icons-react": "^2.47.0",
    "firebase": "^10.7.0",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^6.22.1",
    "reactflow": "^11.11.4",
    "zustand": "^4.5.6",
    "lru-cache": "^10.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "dotenv": "^16.4.7",
    "path": "^0.12.7",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
